{
  "presets": ["react-native"],
  "plugins": [
    "transform-react-jsx-source",
    [
      "module-resolver",
      {
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "app": "./src"
        }
      }
    ],
    [
      "./env-loader",
      {
        "include": ["API_URL", "PLACES_KEY"]
      }
    ]
  ]
}
